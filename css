  0% {
    opacity: .1;
    transform: rotate(0) scale(0.5) skew(1deg);
  }
  30% {
    opacity: .5;
    transform: rotate(0) scale(0.7) skew(1deg);
  }
  100% {
    opacity: .6;
    transform: rotate(0) scale(1) skew(1deg);
  }
}
